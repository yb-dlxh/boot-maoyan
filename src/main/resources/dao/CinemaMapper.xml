<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bootmaoyan.dao.CinemaMapper">

    <select id="selectAll" resultType="Cinema">
        select * from cinema;
    </select>

    <select id="showByID" resultType="Cinema" parameterType="int">
        select * from cinema where id=#{id};
    </select>

</mapper>